<body>
    <div class="jumbotron">
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <h3>All Enquiry</h3>
                    <button class="btn btn-success" data-title="Create" data-toggle="modal" data-target="#create">Enquiry</button>
                    <div class="table-responsive">

                        <table id="mytable" class="table table-striped table-responsive">
                            <thead class="thead-dark">
                            <th scope="col">ID</th>
                            <th scope="col">EnquiryName</th>
                            <th scope="col">Course Name</th>
                            <th scope="col">Other</th>
                            <th scope="col">Date Of Enq</th>
                            <th scope="col">Followup Date</th>
                            <th scope="col">Remark</th>

                            </thead>
                            <tbody id="enquirybody"></tbody>

                        </table>
                        <div class="clearfix"></div>
                        <ul class="pagination pull-right">
                            <li class="disabled"><a href="#"><span class="glyphicon glyphicon-chevron-left"></span></a></li>
                            <li class="active"><a href="#">1</a></li>
                            <li><a href="#">2</a></li>
                            <li><a href="#">3</a></li>
                            <li><a href="#">4</a></li>
                            <li><a href="#">5</a></li>
                            <li><a href="#"><span class="glyphicon glyphicon-chevron-right"></span></a></li>
                        </ul>

                    </div>

                </div>
            </div>
        </div>
    </div>
    <!--Dialog for create course-->
    <form data-toggle="validator" role="form">
        <div class="modal fade" id="create" tabindex="-1" role="dialog" aria-labelledby="edit" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
                        <h4 class="modal-title custom_align" id="Heading">Enquiry Details</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter Enquiry Name" id="enquiry" required />
                        </div>
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter Course Name" id="equcoursename" required />
                        </div>

                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter Other" id="other" required />
                        </div>

                        <div class="form-group">
                            <input type="datetime" class="form-control" placeholder="Enter Date Of Enquiry" id="enqdate" required />
                        </div>

                        <div class="form-group">
                            <input type="datetime" class="form-control" placeholder="Enter Follow Up Date" id="followdate" required />
                        </div>

                        <div class="form-group">
                            <input type="tel" class="form-control" placeholder="Enter Remark" id="remark" required />
                        </div>
                    </div>
                    <div class="modal-footer ">
                        <button type="submit" onclick="CreateEnquiry()" class="btn btn-warning btn-lg" style="width: 100%;"><span class="glyphicon glyphicon-ok-sign"></span> Create</button>
                    </div>
                </div>
                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>

        @*<!--Dialog for Edit course-->
        <div class="modal fade" id="edit" tabindex="-1" role="dialog" aria-labelledby="edit" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
                        <h4 class="modal-title custom_align" id="Heading">Edit Your Detail</h4>
                    </div>
                    <div class="modal-body">
                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter Bank Name" id="editbankname" required />
                        </div>

                        <div class="form-group">
                            <input type="number" class="form-control" placeholder="Enter Enter Account No" id="editaccountno" required />
                        </div>

                        <div class="form-group">
                            <input type="text" class="form-control" placeholder="Enter Address" id="editaddress" required />
                        </div>

                        <div class="form-group">
                            <input type="number" class="form-control" placeholder="Enter Contact No.1" id="editcontact1" required />
                        </div>

                        <div class="form-group">
                            <input type="number" class="form-control" placeholder="Enter Contact No.2" id="editcontact2" required />
                        </div>
                    </div>
                    <div class="modal-footer ">
                        <button type="submit" onclick="CourseTypeEdit()" class="btn btn-warning btn-lg" style="width: 100%;"><span class="glyphicon glyphicon-ok-sign"></span>Update</button>
                    </div>
                </div>
                <!-- /.modal-content -->
            </div>
            <!-- /.modal-dialog -->
        </div>*@

        @*<!--Dialog for delete-->
            <div class="modal fade" id="delete" tabindex="-1" role="dialog" aria-labelledby="edit" aria-hidden="true">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"><span class="glyphicon glyphicon-remove" aria-hidden="true"></span></button>
                            <h4 class="modal-title custom_align" id="Heading">Delete this entry</h4>
                        </div>
                        <div class="modal-body">
                            <div class="alert alert-danger"><span class="glyphicon glyphicon-warning-sign"></span> Are you sure you want to delete this Course Type Record?</div>
                        </div>
                        <div class="modal-footer ">
                            <button type="button" class="btn btn-success" onclick="CourseTypeDelete()"><span class="glyphicon glyphicon-ok-sign"></span> Yes</button>
                            <button type="button" class="btn btn-default" data-dismiss="modal"><span class="glyphicon glyphicon-remove"></span> No</button>
                        </div>
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>*@
    </form>
</body>
</html>
