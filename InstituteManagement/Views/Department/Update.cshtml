
@{
    ViewBag.Title = "Update Departments";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Departmet</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap-flatly.css" rel="stylesheet" />

</head>
<body>
    <div class="jumbotron">
        <h2>Upadate Departmet</h2>
        <form action="">
            <table class="table-responsive">
                <tr class="form-group">
                    <td>
                        <label for="text">Departmet Name</label>
                        <input type="text" class="form-control" id="first" name="firstname">
                    </td>
                   
                </tr>
                <tr class="form-group">
                   
                    <td>
                        <label for="datetime">Department Details</label>
                        <textarea class="form-control" id="address" placeholder="Enter Department Details" name="addr"></textarea>

                    </td>
                </tr>
                </table>
        </form>

    </div>
</body>
</html>

